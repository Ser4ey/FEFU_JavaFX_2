<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="214.0" prefWidth="299.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fefu_javafx_2.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <ImageView fitHeight="165.0" fitWidth="165.0">
    <image>
      <Image url="@images/5f26c657bd1c9.jpg" />
    </image>
  </ImageView>
  <CheckBox mnemonicParsing="false" text="CheckBox" />

  <Label fx:id="welcomeText" />
  <Button fx:id="Button1" mnemonicParsing="false" text="Button">
    <font>
      <Font name="Alef Bold" size="18.0" />
    </font>
    <VBox.margin>
      <Insets right="19.0" />
    </VBox.margin>
  </Button>
  <Button onAction="#onHelloButtonClickTask" text="Hello!" />
</VBox>
